  "main": "index.ts",
  "scripts": {
    "start": "pnpm tailwind && pnpm exec tsx index.ts",
    "eslint": "pnpm exec eslint . --fix",
    "eslint:check": "pnpm exec eslint . ",
    "prettier": "pnpm exec prettier --write .",
    "prettier:check": "pnpm exec prettier --check .",
    "build": "pnpm exec tsc --build",
    "dev": "pnpm exec nodemon",
    "tailwind": "pnpm exec tailwindcss -i src/Web/Public/input.css -o src/Web/Public/output.css",
    "tailwind:watch": "pnpm exec tailwindcss -i src/Web/Public/input.css -o src/Web/Public/output.css --watch",
    "translations": "pnpm exec tsx scripts/translations.ts",
    "json": "pnpm exec tsx scripts/sortJson.ts",
    "data": "pnpm exec tsx scripts/data.ts",
    "data:update": "pnpm data --update",
    "debug": "pnpm exec tsx scripts/debug.ts",
    "convert-minecraft-commands": "pnpm exec tsx scripts/convertMinecraftCommands.ts"
  },
  "keywords": [],
