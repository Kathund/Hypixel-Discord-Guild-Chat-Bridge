    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@eslint/json": "^0.13.2",
    "@j4cobi/eslint-plugin-sort-imports": "^1.0.2",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@types/ejs": "^3.1.5",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.2",
    "@typescript-eslint/utils": "^8.46.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
